\usepackage{pstricks}
\usepackage{xcolor,colortbl}


\makeatletter
\def\thickhrulefill{\leavevmode \leaders \hrule height 1ex \hfill \kern \z@}
\def\@makechapterhead#1{%
  \reset@font
  \parindent \z@ 
  \vspace*{10\p@}%
  \hbox{%
    \vbox{%
      \hsize=2cm%
      \scshape \strut \@chapapp{} \\
      \vrule height 0pt depth 0pt width 10pt%
       \begin{tabular}{c}
         \rowcolor{masterColor}\\
         \arrayrulecolor{masterColor}\hline
         \rowcolor{masterColor}{\white \centering \LARGE \bfseries \thechapter} \\
         \rowcolor{masterColor}\\
         \rowcolor{masterColor}\\
         \rowcolor{masterColor}\\
         \rowcolor{masterColor}\includegraphics[width=0.85cm]{figures/template/logo-Chip.png}\\         
       \end{tabular}%
      }%
    \vbox{%
      \advance\hsize by -2cm
      {\color{masterColor}\hrule height 0.8pt depth 0pt width \hsize}
      \par
      \vskip 6pt%
      \hspace{20pt}%
      \parbox{360pt}{%
        \Huge \bfseries \begin{flushleft} #1\end{flushleft}}%
      }%
    }%
  \vskip 25\p@
  \thispagestyle{chapterFooterStyle}
}
\def\@makeschapterhead#1{%
  \reset@font
  \parindent \z@ 
  \vspace*{10\p@}%
  \hbox{%
    \vbox{%
      \hsize=2cm%
      \begin{tabular}{c}
         \rowcolor{masterColor}\\
         \arrayrulecolor{masterColor}\hline
         \rowcolor{masterColor}\\
         \rowcolor{masterColor}\\
         \rowcolor{masterColor}\\
         \rowcolor{masterColor}\\
         \rowcolor{masterColor}\includegraphics[width=0.85cm]{figures/template/logo-Chip.png}\\  
      \end{tabular}%
      }%
    \vbox{%
      \advance\hsize by -2cm
      {\color{masterColor}\hrule height 0.8pt depth 0pt width \hsize}
      \par
      \vskip 6pt%
      \hspace{20pt}%
      \parbox{360pt}{%
        \Huge \bfseries \begin{flushleft} #1 \end{flushleft}}%
      }%
    }%
  \vskip 75\p@
  \thispagestyle{chapterFooterStyle}
  
}
