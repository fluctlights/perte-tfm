%%% HEADER %%%
\def \headerStyle{
\fancyhead{}       % Clean header


% Chapter to right in odd pages
\fancyhead[RO]{{\mdseries\footnotesize\leftmark}\hspace*{1cm}{\mdseries\bfseries[~\thepage~]}}    
% Section to left in even pages
\fancyhead[LE]{{\mdseries\bfseries[~\thepage~]}\hspace*{1cm}{\mdseries\footnotesize\rightmark}}
}
